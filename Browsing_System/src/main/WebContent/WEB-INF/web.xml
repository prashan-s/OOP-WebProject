<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" version="6.0">
  <display-name>Browsing_System</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <context-param>
    <param-name>dbUrl</param-name>
    <param-value>jdbc:mysql://localhost:3306/OnlineWatchingMoviesandTVSeries</param-value>
  </context-param>
  <context-param>
    <param-name>dbUsername</param-name>
    <param-value>root</param-value>
  </context-param>
  <context-param>
    <param-name>dbPassword</param-name>
    <param-value></param-value>
  </context-param>
  
  
  
  
  
  <servlet>
    <description></description>
    <display-name>MovieController</display-name>
    <servlet-name>MovieController</servlet-name>
    <servlet-class>com.bs.controller.MovieController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MovieController</servlet-name>
    <url-pattern>/MovieController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>MovieDetailsController</display-name>
    <servlet-name>MovieDetailsController</servlet-name>
    <servlet-class>com.bs.controller.MovieDetailsController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MovieDetailsController</servlet-name>
    <url-pattern>/MovieDetailsController</url-pattern>
  </servlet-mapping>
  
  
  
  <servlet>
  	<servlet-name>SelectMovieDB</servlet-name>
    <servlet-class>com.bs.servlet.MovieServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SelectMovieDB</servlet-name>
    <url-pattern>/SelectMovie</url-pattern>
  </servlet-mapping>
  
  
</web-app>